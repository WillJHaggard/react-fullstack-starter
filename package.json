{
  "name": "react-fullstack-starter",
  "version": "0.1.0",
  "description": "Starter Application Using React",
  "main": "backend/server.js",
  "engines": {
    "node": "4.2.2",
    "npm": "2.14.7"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "source .production; webpack --config webpack.config-prod.js --progress --profile --colors",
    "dev": "source .development; webpack-dev-server --config webpack.config-dev.js --progress --colors --port 2992 --inline",
    "node": "source .production; babel-node backend/server.js",
    "nodemon": "source .development; nodemon backend/server.js",
    "release-patch": "git checkout master && npm version patch && git push origin master --tags",
    "release-minor": "git checkout master && npm version minor && git push origin master --tags",
    "release-major": "git checkout master && npm version major && git push origin master --tags",
    "release-as-lib": "git checkout master && npm run build && git commit -am 'Distribute' && npm version major && git push origin master --tags",
    "mocha": "source .testing; mocha --opts specs/mocha.opts specs",
    "test": "npm run mocha -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillJHaggard/react-fullstack-starter.git"
  },
  "keywords": [],
  "author": "Will J. Haggard <willjhaggard@gmail.com> (http://willjhaggard.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WillJHaggard/react-fullstack-starter/issues"
  },
  "homepage": "https://github.com/WillJHaggard/react-fullstack-starter#readme"
}
